{
    "author": "Moacyr Catani, Jr.",
    "name": "package-callback-handler",
    "description": "Handlers to deal with callback functions",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "callback",
        "callbacks",
        "parallel",
        "sequential"
    ],
    "homepage": "https://github.com/moacyr-catani/node-package-callback-handler#readme",
    "bugs": {
        "url": "https://github.com/moacyr-catani/node-package-callback-handler/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moacyr-catani/node-package-callback-handler.git"
    },
    "type": "module",
    "main": "./lib/cjs/index.js",
    "types": "./lib/cjs/types/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/types/index.d.ts",
                "default": "./lib/esm/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/types/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    },
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "build": "npm run clean && npm run build:esm",
        "build2": "npm run clean && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p ./configs/tsconfig.esm.json",
        "build:esm2": "tsc -p ./configs/tsconfig.esm.json && shx mv lib/esm/index.js lib/esm/index.mjs",
        "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
        "build:local": "tsc -p ./configs/tsconfig.local.json",
        "clean": "shx rm -rf ./lib",
        "copy": "shx rm -rf ./lib",
        "examples": "tsx ./examples/index.ts",
        "prepack": "npm run build",
        "test": "jest --config=./configs/jest.config.ts ./tests --verbose --coverage"
    },
    "devDependencies": {
        "@inquirer/prompts": "^7.3.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.5",
        "jest": "^29.7.0",
        "shx": "^0.3.4",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.3",
        "typescript": "^5.7.3"
    }
}
